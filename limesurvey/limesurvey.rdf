<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/fossr/ontology/limesurvey/"
     xml:base="https://w3id.org/fossr/ontology/limesurvey/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="https://w3id.org/fossr/ontology/limesurvey">
        <owl:versionIRI rdf:resource="https://w3id.org/fossr/limesurvey/ontology/0.3/"/>
        <rdfs:comment xml:lang="en">The Survey Ontology provides a formal framework for modeling survey activities, starting from defining a new survey, a group of participants, and related components. It defines key elements in a survey—Question, Variable, Participant, and UserAnswer—and their relationships. Supporting classes include Language (survey language), Condition (defines conditions for displaying questions), and Attribute (describes participant characteristics).

This ontology supports reasoning, querying, and analysis of survey definitions and data report, enabling applications in social sciences and data analysis.</rdfs:comment>
        <rdfs:label xml:lang="en">Survey Ontology</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/fossr/ontology/limesurvey/derivedFrom -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/derivedFrom">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/SurveyVersion"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/SurveyVersion"/>
        <rdfs:comment xml:lang="en">Connects a SurveyVersion to the earlier version it is based on. Enables recursive version lineage among survey versions.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasAnswerOption -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasAnswerOption">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/Question"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/AnswerOption"/>
        <rdfs:comment xml:lang="en">Links a Question to one of its possible answer choices.</rdfs:comment>
        <rdfs:label xml:lang="en">has answer option</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasAttribute -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasAttribute">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/Question"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionAttribute"/>
        <rdfs:comment xml:lang="en">Links a Question to its technical attributes.</rdfs:comment>
        <rdfs:label xml:lang="en">has attribute</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasAttributeLang -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasAttributeLang">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/Question"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionAttributeLang"/>
        <rdfs:comment xml:lang="en">Links a Question to its language-specific attributes.</rdfs:comment>
        <rdfs:label xml:lang="en">has language attribute</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasDefaultValue -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasDefaultValue">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/Question"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/DefaultValue"/>
        <rdfs:comment xml:lang="en">Links a Question to its default value.</rdfs:comment>
        <rdfs:label xml:lang="en">has default value</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasFirstQuestionStep -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasFirstQuestionStep">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionFlow"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionStep"/>
        <rdfs:comment xml:lang="en">Relates a QuestionFlow to its first QuestionFlowStep, representing the initial question in the sequence. This property identifies the starting point of a question-level flow within a survey or question group.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasGroupFlow -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasGroupFlow">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/SurveyVersion"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/GroupFlow"/>
        <rdfs:comment>Connects a SurveyVersion to it corresponding GroupFlow, which defines the process and order in which groups of questions are presented. This property enables the representation of the internal logic, sequence, or branching structure of a survey instrument.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasGroupFlowFirstStep -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasGroupFlowFirstStep">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/GroupFlow"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/GroupFlowStep"/>
        <rdfs:comment xml:lang="en">Relates a GroupFlow to its first GroupFlowStep, representing the fist stage in the sequence of question groups presented in a survey. This property identifies the intial point of the survey flow. It enables explicit modeling of flow completion and supports reasoning over ordered or conditional survey structures.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasGroupFlowLastStep -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasGroupFlowLastStep">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/GroupFlow"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/GroupFlowStep"/>
        <rdfs:comment>Relates a GroupFlow to its final GroupFlowStep, representing the last stage in the sequence of question groups presented in a survey. This property identifies the terminal point of the survey flow, where no subsequent step follows. It enables explicit modeling of flow completion and supports reasoning over ordered or conditional survey structures.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasGroupFlowNextStep -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasGroupFlowNextStep">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/GroupFlowStep"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/GroupFlowStep"/>
        <rdfs:comment xml:lang="en">Relates a GroupFlowStep to the next step within the same GroupFlow. This property defines the sequential order of steps specific to a particular flow, supporting the modeling of linear or conditional navigation of question groups in a survey.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasGroupFlowPreviusStep -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasGroupFlowPreviusStep">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/GroupFlowStep"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/GroupFlowStep"/>
        <rdfs:comment xml:lang="en">Relates a GroupFlowStep to the preceding step within the same GroupFlow. This property represents the step that immediately comes before the current step in the flow, supporting backward navigation and the modeling of sequential or conditional survey logic.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasGroupFlowStep -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasGroupFlowStep">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/GroupFlow"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/GroupFlowStep"/>
        <rdfs:comment xml:lang="en">An object property indicating that a GroupFlow is composed of one or more GroupFlowStep instances. Each step represents a stage in the survey flow, typically corresponding to the display of a GroupQuestion. The property enables the representation of ordered, conditional, or branching sequences of question groups.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasInstrument -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasInstrument">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/Study"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/Survey"/>
        <rdfs:comment>The object property hasInstrument links the study to the survey instrument that operationalizes the investigation.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasIntervalValidity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasIntervalValidity">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/SurveyVersion"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/TimeInterval"/>
        <rdfs:comment xml:lang="en">Relates a SurveyVerion to its time Interval of validitiy. This property identifies the interval when the survey is valid</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasLastQuestionStep -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasLastQuestionStep">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionFlow"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionStep"/>
        <rdfs:comment xml:lang="en">Relates a QuestionFlow to its final QuestionFlowStep, representing the last question in the sequence. This property identifies the terminal point of the question-level flow.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasNextQuestionStep -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasNextQuestionStep">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionStep"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionStep"/>
        <rdfs:comment xml:lang="en">Relates a QuestionStep to the next QuestionStep in the sequence of a QuestionFlow. This property defines the forward navigation between questions, supporting linear or conditional progression within a survey.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasParentQuestion -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasParentQuestion">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/Question"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/Question"/>
        <rdfs:comment xml:lang="en">Links a subquestion to its parent question (for nested/matrix questions).</rdfs:comment>
        <rdfs:label xml:lang="en">has parent question</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasPreviusQuestionStep -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasPreviusQuestionStep">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionStep"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionStep"/>
        <rdfs:comment xml:lang="en">Relates a QuestionStep to the preceding QuestionStep in the sequence of a QuestionFlow. This property defines backward navigation and supports reasoning about the order of questions.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasQuestion -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasQuestion">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionStep"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/Question"/>
        <rdfs:comment xml:lang="en">Relates a QuestionStep to the Question it presents. This property indicates which question is displayed at a given step in a QuestionFlow, linking the procedural flow structure to the actual survey content.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasQuestionFlow -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasQuestionFlow">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionGroup"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionFlow"/>
        <rdfs:comment xml:lang="en">Relates a GroupQuestion to a QuestionFlow that defines the sequence and conditional presentation of its constituent questions. This property allows a question group to specify the internal navigation, branching, or ordering of individual QuestionItems within the group.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasQuestionGroup -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasQuestionGroup">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/GroupFlowStep"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionGroup"/>
        <rdfs:comment xml:lang="en">Relates a GroupFlowStep to a QuestionGroup that defines the group contain its questions. This property allows for a specified group flow step to link the specified group.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasQuestionStep -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasQuestionStep">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionFlow"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionStep"/>
        <rdfs:comment xml:lang="en">Represents an individual step in a QuestionFlow, corresponding to the presentation of a single QuestionItem. Each QuestionStep defines its position, sequence, or conditional transition in the flow. Multiple QuestionStep instances together form the complete question-level navigation within a survey or survey section.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasSurveyVersion -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasSurveyVersion">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/Survey"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/SurveyVersion"/>
        <rdfs:comment xml:lang="en">Relates a survey to one of its specific versions. The property is used to indicate that a given survey has produced or been instantiated as one or more survey versions, each corresponding to a particular period, cycle, or methodological update. Enables modeling of longitudinal or repeated survey programs over time.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasType">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/Question"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionType"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/hasVariable -->

    <owl:ObjectProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/hasVariable">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/Question"/>
        <rdfs:range rdf:resource="https://w3id.org/fossr/ontology/limesurvey/Variable"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/fossr/ontology/grelevance -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/fossr/ontology/grelevance">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/GroupFlowStep"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">group condition, if the condition is true the group question will be visible during the survey</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/fossr/ontology/groupdescription -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/fossr/ontology/groupdescription">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionGroup"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">group question description</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/fossr/ontology/groupname -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/fossr/ontology/groupname">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionGroup"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">group name</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/answerCode -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/answerCode">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/AnswerOption"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">Internal code for the answer (e.g., AO01, AO02).</rdfs:comment>
        <rdfs:label xml:lang="en">answer code</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/answerOrder -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/answerOrder">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/AnswerOption"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment xml:lang="en">Display order of this answer option.</rdfs:comment>
        <rdfs:label xml:lang="en">answer order</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/answerScaleId -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/answerScaleId">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/AnswerOption"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment xml:lang="en">Scale identifier for this answer option.</rdfs:comment>
        <rdfs:label xml:lang="en">answer scale ID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/answerText -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/answerText">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/AnswerOption"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">The text displayed for this answer option.</rdfs:comment>
        <rdfs:label xml:lang="en">answer text</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/assessmentValue -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/assessmentValue">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/AnswerOption"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment xml:lang="en">Numeric value used for assessment/scoring.</rdfs:comment>
        <rdfs:label xml:lang="en">assessment value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/attributeAnswerOrder -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/attributeAnswerOrder">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionAttribute"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Order in which answers are displayed (normal/random).</rdfs:comment>
        <rdfs:label xml:lang="en">answer order attribute</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/attributeArrayFilter -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/attributeArrayFilter">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionAttribute"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label xml:lang="en">array filter</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/attributeCssClass -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/attributeCssClass">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionAttribute"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="en">Custom CSS class for styling.</rdfs:comment>
        <rdfs:label xml:lang="en">CSS class</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/attributeHidden -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/fossr/ontology/limesurvey/attributeHidden">
        <rdfs:domain rdf:resource="https://w3id.org/fossr/ontology/limesurvey/QuestionAttribute"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment xml:lang="en">Whether the question is hidden from respondents.</rdfs:comment>
        <rdfs:label xml:lang="en">is hidden</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/fossr/ontology/limesurvey/attributeHideTip -->

