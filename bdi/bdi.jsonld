  "http://www.w3.org/2002/07/owl#inverseOf" : [ {
    "@id" : "https://w3id.org/fossr/ontology/bdi/isDesireProcessOf"
  } ]
}, {
  "@id" : "https://w3id.org/fossr/ontology/bdi/hasEndTime",
