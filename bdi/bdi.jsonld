  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Defines the temporal istant by specifying when a TimeInterval concludes. This is essential for modeling the execution timeframe of an agent’s plans, actions, or mental states."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "https://w3id.org/fossr/ontology/bdi/TimeInterval"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "has end time"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
    "@id" : "https://w3id.org/fossr/ontology/bdi/TimeIstant"
  } ]
}, {
