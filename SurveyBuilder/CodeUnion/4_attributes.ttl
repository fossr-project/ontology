@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ls: <https://w3id.org/fossr/ontology/limesurvey/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# ============================================
# LOGICAL SOURCE - ATTRIBUTES
# ============================================

<#AttributesSource>
  rml:source "attributes_only.json" ;
  rml:referenceFormulation ql:JSONPath ;
  rml:iterator "$[*]" .

# ============================================
# MAPPING ATTRIBUTES DINAMICI
# ============================================

<#DynamicAttributeMapping>
  rml:logicalSource <#AttributesSource> ;

  rr:subjectMap [
    rr:template "https://w3id.org/fossr/data/attribute_dynamic_{parent_qid}_{name}" ;
    rr:class ls:ComponentAttribute
  ] ;

  rr:predicateObjectMap [
    rr:predicate ls:componentName ;
    rr:objectMap [
      rml:reference "name"
    ]
  ] ;

  rr:predicateObjectMap [
    rr:predicate ls:componentValue ;
    rr:objectMap [
      rml:reference "value"
    ]
  ] .
